: {\r\n    ...mapActions(\"product\", [\"removeCart\"]),\r\n    calcPrice() {\r\n      this.cart.forEach((element) => {\r\n        this.totalPrice += element.price * element.quantity;\r\n      });\r\n    },\r\n    checkout() {\r\n      const vm = this;\r\n      setTimeout(() => {\r\n        vm.removeCart();\r\n        alert(\"Purchase successful!\");\r\n        vm.$router.push(\"/\");\r\n      }, 2000);\r\n    },\r\n  },\r\n  mounted() {\r\n    this.calcPrice();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n  .container{\r\n    max-width: 100%;\r\n    max-height: 100%;\r\n  }\r\n\r\n.head-1{\r\nfont-size: 15px;\r\nfont-weight: bold;\r\nmargin:auto;\r\ncolor: darkgrey;\r\n}\r\n.head-2{\r\n  text-align: center;\r\n  margin: auto;\r\n  font-weight: bolder;\r\n}\r\n</style>"],"sourceRoot":"src/views/home"}]}