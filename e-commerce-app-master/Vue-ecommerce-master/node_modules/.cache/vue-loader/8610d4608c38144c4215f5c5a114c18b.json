r\n      class=\" btn-3 btn btn-primary btn-lg btn-block col-9\"\r\n    >ADD TO CART</button>\r\n    <button\r\n      v-else\r\n      @click.stop=\"removeCart(product.id)\"\r\n      type=\"button\"\r\n      class=\"btn btn-primary btn-lg btn-block col-9\"\r\n    >REMOVE FROM CART</button>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapActions, mapState } from \"vuex\";\r\nexport default {\r\n  props: [\"product\"],\r\n  data() {\r\n    return {\r\n      isInCardProp: false,\r\n      quantity: 1,\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapState(\"product\", [\"cart\"]),\r\n  },\r\n  methods: {\r\n    ...mapActions(\"product\", [\"addCart\", \"removeCart\"]),\r\n\r\n    isInCart(id) {\r\n      for (let index = 0; index < this.cart.length; index++) {\r\n        const element = this.cart[index];\r\n        if (element.id === id) {\r\n          return true;\r\n        }\r\n      }\r\n      return false;\r\n    },\r\n  },\r\n  watch: {\r\n    product(val) {\r\n      this.isInCardProp = this.isInCart(val.id);\r\n    },\r\n    cart() {\r\n      this.isInCardProp = this.isInCart(this.product.id);\r\n    },\r\n    quantity(val) {\r\n      if (val <= 0) {\r\n        this.quantity = 1;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n  .btn-3{\r\n    border-radius: 0px;\r\n  }\r\n \r\n</style>"]}]}